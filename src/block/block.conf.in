[MONITOR1]
#path will be superceded by macro
#path=@TZ_USER_CONTENT@
path=/opt/usr/media
mode=WRITE
include=DOWNLOADABLE
exclude=SLP_debug
exclude_proc=mtp-responder
exclude_proc=testmode
logging=5
configend=NULL
